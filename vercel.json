{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://mameyacinendiaye.vercel.app/:path*",
      "permanent": true
    }
  ]
}
